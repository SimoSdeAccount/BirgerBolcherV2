<Window x:Class="BirgerBolcherV2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BirgerBolcherV2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top"  Orientation="Horizontal" Height="40" Width="200"  >
            <Button Content="Nyt Bolche" Click="NytBolcheKlik" Width="100"></Button>
            <Button Content="Vis bolcher" Click="VisBolcheKlik" Width="100" RenderTransformOrigin="3.74,0.7" Height="40" VerticalAlignment="Bottom"></Button>
        </StackPanel>
        <Frame x:Name="ContentWindow" Margin="0,40,0,0" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
